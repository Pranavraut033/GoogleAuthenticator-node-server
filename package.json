{
  "name": "googleauthenticator---node",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "set NODE_ENV=production & (pm2 start --env production || node dist/index.bundle.js)",
    "serve": "set NODE_ENV=development & nodemon --exec babel-node src/bin/www.js",
    "build": "webpack --config ./build/webpack.prod.js",
    "buildDev": "webpack --config ./build/webpack.dev.js",
    "lint": "eslint \"./**/*.js\" --quiet",
    "lintFull": "eslint \"./**/*.js\"",
    "lintFix": "eslint --fix './**/*.js'"
  },
  "keywords": [
    "Google Authenicator",
    "2fa",
    "Express"
  ],
  "author": "Pranav Raut <hello@pranavraut.me>",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "morgan": "^1.10.0",
    "otplib": "^12.0.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "airbnb": "^0.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.5",
    "prettier": "^2.0.5",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  }
}
